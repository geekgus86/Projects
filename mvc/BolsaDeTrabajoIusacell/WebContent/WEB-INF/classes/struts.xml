<struts>
    <constant name="struts.devMode" value="false" />
    <constant name="struts.configuration.xml.reload" value="true" />
   
    <package name="demo-struts" extends="struts-default" namespace="/">
    	<result-types>
           <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
       </result-types>
        
        <interceptors>
   			<interceptor name="sesionValida" class="com.iusa.clases.interceptor.LoginInterceptor"></interceptor>
   			<interceptor-stack name="mistack">
   				<interceptor-ref name="sesionValida" />
   			</interceptor-stack>
   		</interceptors>
   		
   		<default-action-ref name="login" />
   		
   		
   		
   		<global-results>
    		 <result name="login">accesoDenegado.jsp</result>
    		 
		</global-results>
		
         <action name="login" class="com.iusa.clases.controllers.PreLogin">
            <result type="tiles">Entrar</result>
        </action>
         
        
        <action name="cerrar" class="com.iusa.clases.controllers.CerrarSesion">
        	<result type="redirect">/login</result>
        </action>
        
        <action name="CuentaRapida" class="com.iusa.clases.controllers.ListaCuentaRapida">
            <result type="tiles">cuentaRapida</result>
        </action>
        
        
        <action name="HomeUsuario">
            <interceptor-ref name="mistack"></interceptor-ref>
            <result type="tiles">homeUsuario</result>
        </action>
        
        <action name="Candidaturas" class="com.iusa.clases.controllers.Candidaturas">
            <interceptor-ref name="mistack"></interceptor-ref>
            <result type="tiles">candidaturas</result>
        </action>
        
        <action name="doLogin" class="com.iusa.clases.controllers.Login">
         <result name="input" type="tiles">Entrar</result>
         <result name="error" type="tiles">Entrar</result>
           <result name="success" type="redirect">perfilVista</result>
         </action>
        
        <action name="doSerch" class="com.iusa.clases.controllers.Busqueda">
          <result name="error" type="tiles">busquedaVacante</result>
          <result type="tiles">busquedaVacante</result>
        </action>
        
        <action name="perfil" class="com.iusa.clases.controllers.perfilUsuario">
            <interceptor-ref name="mistack"></interceptor-ref>
            <result type="tiles">homeUsuario</result>
        </action>
        
        <action name="opciones" class="com.iusa.clases.controllers.ListaCuentaRapida">
            <interceptor-ref name="mistack"></interceptor-ref>
            <result type="tiles">opcionesCuenta</result>
        </action>
        
        <action name="perfilVista" class="com.iusa.clases.controllers.perfilUsuario">
            <interceptor-ref name="mistack"></interceptor-ref>
            <result type="tiles">vistaPrevia</result>
        </action>
        
        <action name="crearCuenta" class="com.iusa.clases.controllers.Cuenta">
         <result name="input" type="tiles">cuentaRapida</result>
         <result name="error" type="tiles">cuentaRapida</result>
   <result type="redirect">Confirmacion</result>
        </action>
        
        <action name="Confirmacion" class="com.iusa.clases.controllers.Confirmacion">
            <interceptor-ref name="mistack"></interceptor-ref>
            <result type="tiles">confirmacion</result>
        </action>
        
        <action name="actualizar" class="com.iusa.clases.controllers.ActualizarDatos">
            <interceptor-ref name="mistack"></interceptor-ref>
        	<interceptor-ref name="fileUpload">
             </interceptor-ref>
               <interceptor-ref name="defaultStack"></interceptor-ref>
        	<result name="input">actualizar</result>
        	<result name="success" type="chain">perfil</result>
        </action>
         
         <action name="ActualizarFoto" class="com.iusa.clases.controllers.ActualizarFoto">
        	<interceptor-ref name="fileUpload">
        	  <param name="allowedTypes">image/jpeg,image/gif,image/png,image/bmp</param>
        	  <param name="maximumSize">16777216</param>
             </interceptor-ref>
               <interceptor-ref name="defaultStack"></interceptor-ref>
        	<result name="input" type="redirect">perfil</result>
        	<result name="success" type="redirect">perfil</result>
           </action>
             

        
        <action name="CorreoPass" class="com.iusa.clases.controllers.CorreoPass">
            <result type="redirect">login</result>
        </action>
       
       
     
        <action name="mejora_la_web">
            <result>navegadores.jsp</result>
        </action>
       
       <action name="/">
           <result type="redirectAction">
                <param name="actionName">login</param>
           </result>
       </action>

       
       <action name="perfilVista.jsp">
           <result>navegadores.jsp</result>
       </action>
       
       <action name="busqueda.jsp">
           <result type="redirectAction">
                <param name="actionName">login</param>
           </result>
       </action>
       
       <action name="confirmacion_cuenta.jsp">
           <result type="redirectAction">
                <param name="actionName">login</param>
           </result>
       </action>
       
       <action name="cuenta_datos.jsp">
           <result type="redirectAction">
                <param name="actionName">login</param>
           </result>
       </action>
       
       <action name="login.jsp">
           <result type="redirectAction">
                <param name="actionName">login</param>
           </result>
       </action>
       
       <action name="mis_candidaturas.jsp">
           <result type="redirectAction">
                <param name="actionName">login</param>
           </result>
       </action>
       
       <action name="opciones.jsp">
           <result type="redirectAction">
                <param name="actionName">login</param>
           </result>
       </action>
       
       <action name="usu_home.jsp">
           <result type="redirectAction">
                <param name="actionName">login</param>
           </result>
       </action>
       
		<action name="entrevistaCalidad" class="iusacell.mdc.arh.action.EntrevistaCalidad"
			method="cuestionarioCalidad">
			<result name="entrevistaCalidad">/WEB-INF/iusacell/mdc/arh/entrevistaCalidad.jsp</result>
			<result name="entrevistaCalidadFail">/accesoDenegado.jsp</result>
		</action>

		<action name="obtenerCuestionario" class="iusacell.mdc.arh.action.EntrevistaCalidad"
			method="obtenerCuestionarioSalida">
			<result name="entrevistaCalidad">/WEB-INF/iusacell/mdc/arh/entrevistaCalidad.jsp</result>
			<result name="entrevistaCalidadFail">/accesoDenegado.jsp</result>
		</action>

		<action name="botonGuardarCuestionarioResp" class="iusacell.mdc.arh.action.EntrevistaCalidad"
			method="botonGuardarCuestionario">
			<result name="entrevistaCalidad">/WEB-INF/iusacell/mdc/arh/entrevistaCalidad.jsp</result>
			<result name="entrevistaCalidadFail">/accesoDenegado.jsp</result>
		</action>

	</package>
	
