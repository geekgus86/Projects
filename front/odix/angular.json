{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "fbx-4": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/favicon.ico",
              "src/apple-touch-icon.png",
              "src/robots.txt",
              "src/manifest.json",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/bs-datepicker.css",
              "node_modules/c3/c3.min.css",
              "src/assets/css/icon-font.css",
              "src/main.scss",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.css",
              "src/assets/css/worldmap/worldmap_ammap.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css",
              "src/assets/css/animate.css",

              "node_modules/ag-grid-community/dist/styles/ag-grid.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-balham.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-balham-dark.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-blue.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-dark.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-fresh.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-material.css",
              "node_modules/ag-grid-community/dist/styles/ag-theme-bootstrap.css",
              "node_modules/ngx-ui-switch/ui-switch.component.css"

            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/d3/dist/d3.min.js",
              "node_modules/c3/c3.min.js",
              "node_modules/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js",
              "node_modules/powerbi-client/dist/powerbi.js",
              "src/assets/js/worldmap/worldmap_ammap.js",
              "src/assets/js/worldmap/worldmap_worldLow.js",
              "src/assets/js/worldmap/worldmap_blackTheme.js",
              "src/assets/js/global/ammap.js",
              "src/assets/js/global/worldLow.js",
              "src/assets/js/global/black.js",

              "src/assets/js/amchart/plugins/funnel.js",
              "src/assets/js/amchart/plugins/gantt.js",
              "src/assets/js/amchart/plugins/gauge.js",
              "src/assets/js/amchart/plugins/pie.js",
              "src/assets/js/amchart/plugins/radar.js",
              "src/assets/js/amchart/plugins/serial.js",
              "src/assets/js/amchart/plugins/xy.js",
              "src/assets/js/amchart/themes/chalk.js",
              "src/assets/js/amchart/themes/dark.js",
              "src/assets/js/amchart/themes/light.js",
              "src/assets/js/amchart/themes/patterns.js",
              "src/assets/js/overview/amCharts.js",
              "src/assets/js/overview/serial.js",
              "src/assets/js/overview/light.js",
              "node_modules/wowjs/dist/wow.min.js",
              "src/assets/js/worldmap/jquery.maphilight.min.js",
              "node_modules/leader-line/leader-line.min.js",
              "node_modules/anim-event/anim-event.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fbx-4:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "fbx-4:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fbx-4:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "src/karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/main.scss"
            ],
            "assets": [
              "src/favicon.ico",
              "src/apple-touch-icon.png",
              "src/robots.txt",
              "src/manifest.json",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "fbx-4-e2e": {
      "root": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "fbx-4:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "fbx-4"
}
