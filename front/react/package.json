    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "CI=true node scripts/test.js --env=jsdom",
    "test:coverage": "CI=true node scripts/test.js --env=jsdom --coverage",
    "deploy-testing": "npm run build && scp -r build ubuntu@testing.sensai.net:~/admin/",
    "deploy-etesting": "npm run build && scp -r build ubuntu@etesting.sensai.net:~/admin/",
    "deploy-mtesting": "npm run build && scp -r build ubuntu@mtesting.sensai.net:~/admin/",
    "deploy-production": "npm run build && scp -r build adminmaster@sfms.metalsa.com:~/admin/"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "devDependencies": {
      "babel-core": "^6.26.3",
      "babel-loader": "^7.1.4",
      "babel-preset-env": "^1.7.0",
      "babel-preset-react": "^6.24.1",
      "express": "^4.16.3",
      "nodemon": "^1.18.4",
      "react": "^16.5.0",
      "react-dom": "^16.5.0",
      "webpack": "^4.17.2",
      "webpack-cli": "^3.1.0",
      "webpack-dev-middleware": "^3.3.0",
      "webpack-hot-middleware": "^2.23.1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 1,
        "functions": 1,
        "lines": 1
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "jest": "20.0.4",
    "jest-enzyme": "^6.0.4",
    "jest-fetch-mock": "^1.6.6",
    "sinon": "^6.3.4"
  }
}
