    "react-chartjs-2": "^2.7.4",
    "react-csv": "^1.0.15-RC2",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.5.0",
    "react-icons": "^3.0.5",
    "react-intl": "^2.7.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0",
    "react-tooltip": "^3.10.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve": "1.6.0",
